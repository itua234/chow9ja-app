{
  "expo": {
    "name": "chow9ja",
    "slug": "chow9ja",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ziga.icon.png",
    "scheme": "chow9ja",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/ziga.icon.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ziga.icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.itua234.chow9ja"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ziga.icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#121212"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.255095568241-fbu560u0doll0a1h6arq0avblqiahr1a"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "70c8ccd0-03c0-429f-93c4-219bbf366ca8"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/70c8ccd0-03c0-429f-93c4-219bbf366ca8"
    }
  }
}
